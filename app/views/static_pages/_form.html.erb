<div class="m-5 box">
  <%= form_with url: root_path, method: :get, local: true do |f|%>
  
  <div class="field">
    <%= f.label :user_id, "User id:"%>
    <div class="control">
    <%= f.text_field :user_id%>
    </div>
  </div>
  <div class="field">
    <%= f.submit "Search", class: "button"%>
  </div>
  
<% end %>
</div>
